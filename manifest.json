{
	"background" : {
		"scripts": ["assets/jquery.min.js", "common.js", "background.js"]
	},
	"manifest_version" : 2,
	"name": "FocusOn",
	"version": "1.0",
	"description": "Stop wasting your time",
	"browser_action": {
		"default_icon": {
			"16": "icons/icon-off.png",
			"24": "icons/icon-off.png",
			"32": "icons/icon-off.png"
		},
		"default_title": "Focus On",     
        "default_popup": "popup.html"
	},
 	"content_scripts": [ {
		"js": [ "assets/jquery.min.js", "checktab.js" ],
		"matches": [ "http://*/*", "https://*/*" ],
		"run_at": "document_start"
	} ],
	"icons": {
		"48": "icons/icon-on.png"
	},
	"permissions": ["storage", "tabs", "webRequest", "webRequestBlocking", "*://*/*"],
	"options_page" : "options.html"
}